{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true
	},
	"include": ["**/*.ts"]
}
